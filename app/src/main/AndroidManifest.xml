<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jorgeloayza.tareas5">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.jorgeloayza.tareas5.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jorgeloayza.tareas5.FavoritosActivity"
            android:parentActivityName="com.jorgeloayza.tareas5.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jorgeloayza.tareas5.com.jorgeloayza.tareas5.MainActivity" />
        </activity>
        <activity android:name="com.jorgeloayza.tareas5.AboutActivity"
            android:parentActivityName="com.jorgeloayza.tareas5.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jorgeloayza.tareas5.com.jorgeloayza.tareas5.MainActivity" />
        </activity>
        <activity android:name="com.jorgeloayza.tareas5.ContacoActivity"
            android:parentActivityName="com.jorgeloayza.tareas5.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jorgeloayza.tareas5.com.jorgeloayza.tareas5.MainActivity" />
        </activity>
    </application>

</manifest>